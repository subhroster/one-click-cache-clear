{
  "manifest_version": 3,
  "name": "One-Click Cache Cleaner",
  "version": "1.0",
  "description": "Clear browser cache in one click",
  "permissions": [
    "browsingData",
    "notifications"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_width": 370,     
     "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
